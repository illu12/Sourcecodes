#include <stdio.h>
#include <stdlib.h>

//Create function to display output.
void display(char a[10], float b){
	printf("%s\n", a);
    printf("%f\n", b);
  };
  
main()
{
  int years;
  printf("Amount of years: ");
  scanf("%d",&years);
  
  if(years == 1){
   
  float nettoomsaetning;  
  printf("Input nettoomsaetning: ");
  scanf("%f", &nettoomsaetning);

  float vareforbrug;
  printf("Input vareforbrug: ");
  scanf("%f", &vareforbrug);
  
  float bruttofortjeneste = nettoomsaetning-vareforbrug;
  printf("Input bruttofortjeneste: %f\n",bruttofortjeneste);
    
  float andre_eksterne_omk;
  printf("Input andre eksterne omkostninger: ");
  scanf("%f", &andre_eksterne_omk);

  float personale_omk;
  printf("Input personale omkostninger: ");
  scanf("%f", &personale_omk);

  float resultat_foer_afskrivninger = bruttofortjeneste-(andre_eksterne_omk+personale_omk);
  printf("Input resultat foer afskrivninger: %f\n",resultat_foer_afskrivninger);

  float afskrivninger;
  printf("Input afskrivninger: ");
  scanf("%f", &afskrivninger);
  
  float resultat_primaer_drift = resultat_foer_afskrivninger-afskrivninger;
  printf("Input resultat af primaer drift: %f\n",resultat_primaer_drift);  

  float finansielle_ind;
  printf("Input finansielle indtaegter: ");
  scanf("%f", &finansielle_ind);

  float finansielle_omk;
  printf("Input finansielle omkostninger: ");
  scanf("%f", &finansielle_omk);

  float resultat_foer_skat = resultat_primaer_drift+finansielle_ind-finansielle_omk;
  printf("Input resultat foer skat: %f\n",resultat_foer_skat);
  
  float skat_af_aarets_resultat;
  printf("Input skat af aaret resultat: ");
  scanf("%f", &skat_af_aarets_resultat);
  
  float aarets_resultat = resultat_foer_skat-skat_af_aarets_resultat;
  printf("Input af aarets resultat: %f\n",aarets_resultat);

  float gennemsnitlige_aktiver;
  printf("Input gennemsnitlige aktiver: ");
  scanf("%f",&gennemsnitlige_aktiver);

  float gennemsnitlige_egenkapital;
  printf("Input gennemsnitlige egenkapital: ");
  scanf("%f",&gennemsnitlige_egenkapital);
  
  float gennemsnitlige_forpligtelser;
  printf("Input gennemsnitlige forpligtelser: ");
  scanf("%f",&gennemsnitlige_forpligtelser);

  /*-----------------------------------------------------*/

  float AG_calc = resultat_primaer_drift/gennemsnitlige_aktiver*100;
  float OG_calc = resultat_primaer_drift/nettoomsaetning*100;
  float AOH_calc = nettoomsaetning/gennemsnitlige_aktiver;
  float EKF_calc = resultat_foer_skat/gennemsnitlige_egenkapital*100;
  float GR_calc = finansielle_omk/gennemsnitlige_forpligtelser*100;
  float Gearing_calc = gennemsnitlige_forpligtelser/gennemsnitlige_egenkapital;
  
  /*-----------------------------------------------------*/  
  
  struct desired_table{
    char name[10];
	float val;
  };
  
  struct desired_table AG;
  strcpy(AG.name, "AG");
  AG.val = AG_calc;
  
  struct desired_table OG;
  strcpy(OG.name, "OG");
  OG.val = OG_calc;
  
  struct desired_table AOH;
  strcpy(AOH.name, "AOH");
  AOH.val = AOH_calc;
  
  struct desired_table EKF;
  strcpy(EKF.name, "EKF");
  EKF.val = EKF_calc;
  
  struct desired_table GR;
  strcpy(GR.name, "GR");
  GR.val = GR_calc;
  
  struct desired_table Gearing;
  strcpy(Gearing.name, "Gearing");
  Gearing.val = Gearing_calc;
  
/*-------------------------------------------------------*/

  printf("--------------------------------------------------------------------\n");
  printf("\t\t\tRentabilitetsanalyse\n");
  printf("--------------------------------------------------------------------\n");
  
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",AG.name,AG.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",OG.name,OG.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",AOH.name,AOH.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",EKF.name,EKF.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",GR.name,GR.val);
  printf("|---------------|\n");
  printf("|%s\t|\t| %f |\n",Gearing.name,Gearing.val);
  printf("|---------------|\n");
  }
  if(years == 2){
   
  float nettoomsaetning;  
  printf("Input nettoomsaetning: ");
  scanf("%f", &nettoomsaetning);

  float vareforbrug;
  printf("Input vareforbrug: ");
  scanf("%f", &vareforbrug);
  
  float bruttofortjeneste = nettoomsaetning-vareforbrug;
  printf("Input bruttofortjeneste: %f\n",bruttofortjeneste);
    
  float andre_eksterne_omk;
  printf("Input andre eksterne omkostninger: ");
  scanf("%f", &andre_eksterne_omk);

  float personale_omk;
  printf("Input personale omkostninger: ");
  scanf("%f", &personale_omk);

  float resultat_foer_afskrivninger = bruttofortjeneste-(andre_eksterne_omk+personale_omk);
  printf("Input resultat foer afskrivninger: %f\n",resultat_foer_afskrivninger);

  float afskrivninger;
  printf("Input afskrivninger: ");
  scanf("%f", &afskrivninger);
  
  float resultat_primaer_drift = resultat_foer_afskrivninger-afskrivninger;
  printf("Input resultat af primaer drift: %f\n",resultat_primaer_drift);  

  float finansielle_ind;
  printf("Input finansielle indtaegter: ");
  scanf("%f", &finansielle_ind);

  float finansielle_omk;
  printf("Input finansielle omkostninger: ");
  scanf("%f", &finansielle_omk);

  float resultat_foer_skat = resultat_primaer_drift+finansielle_ind-finansielle_omk;
  printf("Input resultat foer skat: %f\n",resultat_foer_skat);
  
  float skat_af_aarets_resultat;
  printf("Input skat af aaret resultat: ");
  scanf("%f", &skat_af_aarets_resultat);
  
  float aarets_resultat = resultat_foer_skat-skat_af_aarets_resultat;
  printf("Input af aarets resultat: %f\n",aarets_resultat);

  float gennemsnitlige_aktiver;
  printf("Input gennemsnitlige aktiver: ");
  scanf("%f",&gennemsnitlige_aktiver);

  float gennemsnitlige_egenkapital;
  printf("Input gennemsnitlige egenkapital: ");
  scanf("%f",&gennemsnitlige_egenkapital);
  
  float gennemsnitlige_forpligtelser;
  printf("Input gennemsnitlige forpligtelser: ");
  scanf("%f",&gennemsnitlige_forpligtelser);

  /*-----------------------------------------------------*/

  float AG_calc = resultat_primaer_drift/gennemsnitlige_aktiver*100;
  float OG_calc = resultat_primaer_drift/nettoomsaetning*100;
  float AOH_calc = nettoomsaetning/gennemsnitlige_aktiver;
  float EKF_calc = resultat_foer_skat/gennemsnitlige_egenkapital*100;
  float GR_calc = finansielle_omk/gennemsnitlige_forpligtelser*100;
  float Gearing_calc = gennemsnitlige_forpligtelser/gennemsnitlige_egenkapital;
  
  /*-----------------------------------------------------*/  
  
  struct desired_table{
    char name[10];
	float val;
  };
  
  struct desired_table AG;
  strcpy(AG.name, "AG");
  AG.val = AG_calc;
  
  struct desired_table OG;
  strcpy(OG.name, "OG");
  OG.val = OG_calc;
  
  struct desired_table AOH;
  strcpy(AOH.name, "AOH");
  AOH.val = AOH_calc;
  
  struct desired_table EKF;
  strcpy(EKF.name, "EKF");
  EKF.val = EKF_calc;
  
  struct desired_table GR;
  strcpy(GR.name, "GR");
  GR.val = GR_calc;
  
  struct desired_table Gearing;
  strcpy(Gearing.name, "Gearing");
  Gearing.val = Gearing_calc;
  
/*-------------------------------------------------------*/

  printf("--------------------------------------------------------------------\n");
  printf("\t\t\tRentabilitetsanalyse\n");
  printf("--------------------------------------------------------------------\n");
  
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",AG.name,AG.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",OG.name,OG.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",AOH.name,AOH.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",EKF.name,EKF.val);
  printf("|---------------|\n");
  printf("|%s\t\t|\t| %f |\n",GR.name,GR.val);
  printf("|---------------|\n");
  printf("|%s\t|\t| %f |\n",Gearing.name,Gearing.val);
  printf("|---------------|\n");
  }
  
  
  
  system("PAUSE"); 
}


