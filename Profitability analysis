  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_three,AG_calc_3,OG_calc_3,AOH_calc_3,GR_calc_3,EKF_calc_3,Gearing_calc_3);
  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_four,AG_calc_4,OG_calc_4,AOH_calc_4,GR_calc_4,EKF_calc_4,Gearing_calc_4);
  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_five,AG_calc_5,OG_calc_5,AOH_calc_5,GR_calc_5,EKF_calc_5,Gearing_calc_5);
/*-------------------------------------------------------*/
  }
  system("PAUSE");
}
