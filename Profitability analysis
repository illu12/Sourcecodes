  printf("Input resultat foer skat: %f\n",resultat_foer_skat);
  printf("Input skat af aaret resultat: ");
  scanf("%f", &skat_af_aarets_resultat);
  float aarets_resultat = resultat_foer_skat-skat_af_aarets_resultat;
  printf("Input af aarets resultat: %f\n",aarets_resultat);
