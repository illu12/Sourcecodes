  strcpy(GR.name, "GR");
  GR.val = GR_calc;
  struct desired_table Gearing;
  strcpy(Gearing.name, "Gearing");
  Gearing.val = Gearing_calc;
/*-------------------------------------------------------*/
	printf("--------------------------------------------------------------------\n");
	printf("\t\t\tRentabilitetsanalyse\n");
	printf("--------------------------------------------------------------------\n");
	printf("\nYear\t\t%s\t%s\t%s\t%s\t%s\t%s",AG.name,OG.name,AOH.name,GR.name,EKF.name,Gearing.name);
	printf("\n---------------------------------------------------------------\n");
	printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year,AG.val,OG.val,AOH.val,GR.val,EKF.val,Gearing.val);
/*-------------------------------------------------------*/
  }
  if(years == 2){

	  
  int format_year_one;
