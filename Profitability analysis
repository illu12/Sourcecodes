  float resultat_foer_afskrivninger_5 = bruttofortjeneste_5-(andre_eksterne_omk_5+personale_omk_5);
  printf("Input resultat foer afskrivninger: %f\n",resultat_foer_afskrivninger_5);
  printf("Input afskrivninger: ");
  scanf("%f", &afskrivninger_5);
  float resultat_primaer_drift_5 = resultat_foer_afskrivninger_5-afskrivninger_5;
  printf("Input resultat af primaer drift: %f\n",resultat_primaer_drift_5);  
  printf("Input finansielle indtaegter: ");
  scanf("%f", &finansielle_ind_5);  
  printf("Input finansielle omkostninger: ");
  scanf("%f", &finansielle_omk_5);  
  float resultat_foer_skat_5 = resultat_primaer_drift_5+finansielle_ind_5-finansielle_omk_5;
  printf("Input resultat foer skat: %f\n",resultat_foer_skat_5);
  printf("Input skat af aaret resultat: ");
  scanf("%f", &skat_af_aarets_resultat_5);
  float aarets_resultat_5 = resultat_foer_skat_5-skat_af_aarets_resultat_5;
  printf("Input af aarets resultat: %f\n",aarets_resultat_5);  
/*-----------------------------------------------------*/
// 			Gennemsnitlige aktiver,forp,egenkapital    //
/*-----------------------------------------------------*/  
  //Year 1&2  
  float gennemsnitlige_aktiver_12;
  float gennemsnitlige_forpligtelser_12;
  //Year 2&3
  float gennemsnitlige_aktiver_23;
  float gennemsnitlige_forpligtelser_23;
  
  //Year 3&4
  float gennemsnitlige_aktiver_34;
  float gennemsnitlige_forpligtelser_34;
  
  //Year 4&5
  float gennemsnitlige_aktiver_45;
  float gennemsnitlige_forpligtelser_45;
  
  float a;
  printf("\nInput aktiver year one: ");
  scanf("%f",&a);
  float b;
  printf("Input aktiver year two: ");
  scanf("%f",&b);
  float c;
  printf("Input forpligtelser year one: ");
  scanf("%f",&c);
  float d;
  printf("Input forpligtelser year two: ");
  scanf("%f",&d);
  gennemsnitlige_aktiver_12 = (a+b)/2;
  gennemsnitlige_forpligtelser_12 = (c+d)/2;
  float gennemsnitlige_egenkapital_12 = gennemsnitlige_aktiver_12-gennemsnitlige_forpligtelser_12;
  
  float e;
  printf("\nInput aktiver year two: ");
  scanf("%f",&e);
  float f;
  printf("Input aktiver year three: ");
  scanf("%f",&f);
  float g;
  printf("Input forpligtelser year two: ");
  scanf("%f",&g);
  float h;
  printf("Input forpligtelser year three: ");
  scanf("%f",&h);
  gennemsnitlige_aktiver_23 = (e+f)/2;
  gennemsnitlige_forpligtelser_23 = (g+h)/2;
  float gennemsnitlige_egenkapital_23 = gennemsnitlige_aktiver_23-gennemsnitlige_forpligtelser_23;
  
  float i;
  printf("\nInput aktiver year three: ");
  scanf("%f",&i);
  float j;
  printf("Input aktiver year four: ");
  scanf("%f",&j);
  float k;
  printf("Input forpligtelser year three: ");
  scanf("%f",&k);
  float l;
  printf("Input forpligtelser year four: ");
  scanf("%f",&l);
  gennemsnitlige_aktiver_34 = (i+j)/2;
  gennemsnitlige_forpligtelser_34 = (k+l)/2;
  float gennemsnitlige_egenkapital_34 = gennemsnitlige_aktiver_34-gennemsnitlige_forpligtelser_34;
  
  float m;
  printf("\nInput aktiver year four: ");
  scanf("%f",&m);
  float n;
  printf("Input aktiver year five: ");
  scanf("%f",&n);
  float o;
  printf("Input forpligtelser year four: ");
  scanf("%f",&o);
  float p;
  printf("Input forpligtelser year five: ");
  scanf("%f",&p);
  gennemsnitlige_aktiver_45 = (m+n)/2;
  gennemsnitlige_forpligtelser_45 = (o+p)/2;
  float gennemsnitlige_egenkapital_45 = gennemsnitlige_aktiver_45-gennemsnitlige_forpligtelser_45;
  
/*-----------------------------------------------------*/  
  float AG_calc = resultat_primaer_drift/gennemsnitlige_aktiver_12*100;
  float OG_calc = resultat_primaer_drift/nettoomsaetning*100;
  float AOH_calc = nettoomsaetning/gennemsnitlige_aktiver_12;
  float EKF_calc = resultat_foer_skat/gennemsnitlige_egenkapital_12*100;
  float GR_calc = finansielle_omk/gennemsnitlige_forpligtelser_12*100;
  float Gearing_calc = gennemsnitlige_forpligtelser_12/gennemsnitlige_egenkapital_12; 

  float AG_calc_2 = resultat_primaer_drift_2/gennemsnitlige_aktiver_12*100;
  float OG_calc_2 = resultat_primaer_drift_2/nettoomsaetning_2*100;
  float AOH_calc_2 = nettoomsaetning_2/gennemsnitlige_aktiver_12;
  float EKF_calc_2 = resultat_foer_skat_2/gennemsnitlige_egenkapital_12*100;
  float GR_calc_2 = finansielle_omk_2/gennemsnitlige_forpligtelser_12*100;
  float Gearing_calc_2 = gennemsnitlige_forpligtelser_12/gennemsnitlige_egenkapital_12;
  
  float AG_calc_3 = resultat_primaer_drift_3/gennemsnitlige_aktiver_23*100;
  float OG_calc_3 = resultat_primaer_drift_3/nettoomsaetning_3*100;
  float AOH_calc_3 = nettoomsaetning_3/gennemsnitlige_aktiver_23;
  float EKF_calc_3 = resultat_foer_skat_3/gennemsnitlige_egenkapital_23*100;
  float GR_calc_3 = finansielle_omk_3/gennemsnitlige_forpligtelser_23*100;
  float Gearing_calc_3 = gennemsnitlige_forpligtelser_23/gennemsnitlige_egenkapital_23;  
  
  float AG_calc_4 = resultat_primaer_drift_4/gennemsnitlige_aktiver_34*100;
  float OG_calc_4 = resultat_primaer_drift_4/nettoomsaetning_4*100;
  float AOH_calc_4 = nettoomsaetning_4/gennemsnitlige_aktiver_34;
  float EKF_calc_4 = resultat_foer_skat_4/gennemsnitlige_egenkapital_34*100;
  float GR_calc_4 = finansielle_omk_4/gennemsnitlige_forpligtelser_34*100;
  float Gearing_calc_4 = gennemsnitlige_forpligtelser_34/gennemsnitlige_egenkapital_34;

  float AG_calc_5 = resultat_primaer_drift_5/gennemsnitlige_aktiver_45*100;
  float OG_calc_5 = resultat_primaer_drift_5/nettoomsaetning_5*100;
  float AOH_calc_5 = nettoomsaetning_5/gennemsnitlige_aktiver_45;
  float EKF_calc_5 = resultat_foer_skat_5/gennemsnitlige_egenkapital_45*100;
  float GR_calc_5 = finansielle_omk_5/gennemsnitlige_forpligtelser_45*100;
  float Gearing_calc_5 = gennemsnitlige_forpligtelser_45/gennemsnitlige_egenkapital_45;  
/*-----------------------------------------------------*/
// 			Display output  						   //
/*-----------------------------------------------------*/  
  printf("--------------------------------------------------------------------\n");
  printf("\t\t\tRentabilitetsanalyse\n");
  printf("--------------------------------------------------------------------\n");
  printf("\nYear\tAG\tOG\tAOH\tGR\tEKF\tGearing");
  printf("\n---------------------------------------------------------------\n");
  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_one,AG_calc,OG_calc,AOH_calc,GR_calc,EKF_calc,Gearing_calc);
  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_two,AG_calc_2,OG_calc_2,AOH_calc_2,GR_calc_2,EKF_calc_2,Gearing_calc_2);
  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_three,AG_calc_3,OG_calc_3,AOH_calc_3,GR_calc_3,EKF_calc_3,Gearing_calc_3);
  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_four,AG_calc_4,OG_calc_4,AOH_calc_4,GR_calc_4,EKF_calc_4,Gearing_calc_4);
  printf("%d\t%g\t%g\t%g\t%g\t%g\t%g\n",format_year_five,AG_calc_5,OG_calc_5,AOH_calc_5,GR_calc_5,EKF_calc_5,Gearing_calc_5);
/*-------------------------------------------------------*/
  }
  printf("\n");
  system("PAUSE");
}
